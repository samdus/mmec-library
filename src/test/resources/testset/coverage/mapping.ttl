@prefix isc:  <http://resource.geosciml.org/classifier/ics/ischart/> .
@prefix rdf:  <http://www.w3.org/1999/02/22-rdf-syntax-ns#> .
@prefix rr:   <http://www.w3.org/ns/r2rml#> .
@prefix xsd:  <http://www.w3.org/2001/XMLSchema#> .
@prefix mmec: <http://www.griis.ca/projects/mmec/> .
@prefix obo:  <http://purl.obolibrary.org/obo/> .

<MappingDefinition>
    a                  mmec:MappingDefinition ;
    mmec:OntoRel        'http://purl.obolibrary.org/obo/humanbodyweight-appli.owl/couverture/v1.0/20240313-1437' ;
    mmec:Source         'https://griis.ca/JeDIS' ;
    mmec:TemplatePrefix 'http://www.griis.ca/projects/' .

# ----------------------------------------------------------------------------------------------------------------------
# Conversions
# ----------------------------------------------------------------------------------------------------------------------
<Conversion:TEXT:string>
    a                         mmec:conversion ;
    mmec:conversionInputType  "text" ;
    mmec:conversionOutputType "\"BW\".\"string\"" ;
    mmec:conversionFunction   "convertTextToBWString" ;
    mmec:validationFunction   "v_convertTextToBWString" .
<Conversion:VARCHAR:string>
    a                         mmec:conversion ;
    mmec:conversionInputType  "varchar" ;
    mmec:conversionOutputType "\"BW\".\"string\"" .

# ----------------------------------------------------------------------------------------------------------------------
# Arrimage des différents cas d'exemple
# ----------------------------------------------------------------------------------------------------------------------
#
# NOTE: Avec l'extension, les rr:template seront engendrés automatiquement

# **** Arrimage simple ****

# -- Classes

# "HBW_0000022"@human name
<Mapping:HBW_0000022>
    a                     rr:TriplesMap ;
    rr:logicalTable       [ a           rr:R2RMLView ;
                            rr:sqlQuery "SELECT \"PRENOM\", \"NOM\" FROM \"EXP\".\"PERSONNE\"" ] ;
    rr:subjectMap         [ a           rr:SubjectMap, rr:TermMap ;
                            rr:class    obo:HBW_0000022 ;
                            rr:template "http://purl.obolibrary.org/obo/HBW_0000022/{\"PRENOM\"}/{\"NOM\"}" ;
                            rr:termType rr:IRI ] .

# "IAO_0020015"@personal name
<Mapping:IAO_0020015>
    a                     rr:TriplesMap ;
    rr:logicalTable       [ a           rr:R2RMLView ;
                            rr:sqlQuery "SELECT \"PRENOM\" FROM \"EXP\".\"PERSONNE\"" ] ;
    rr:subjectMap         [ a           rr:SubjectMap, rr:TermMap ;
                            rr:class    obo:IAO_0020015 ;
                            rr:template "http://purl.obolibrary.org/obo/IAO_0020015/{\"PRENOM\"}" ;
                            rr:termType rr:IRI ] .

# "IAO_0020017"@family name
<Mapping:IAO_0020017>
    a               rr:TriplesMap ;
    rr:logicalTable [ a           rr:R2RMLView ;
                      rr:sqlQuery "SELECT \"NOM\" FROM \"EXP\".\"PERSONNE\"" ] ;
    rr:subjectMap   [ a           rr:SubjectMap, rr:TermMap ;
                      rr:class    obo:IAO_0020017 ;
                      rr:template "http://purl.obolibrary.org/obo/IAO_0020017/{\"NOM\"}" ;
                      rr:termType rr:IRI ] .

# -- Propriétés d'objet
# "HBW_0000022"@human name BFO_0000051@'has part' "IAO_0020015"@personal name
<Mapping:HBW_0000022>
    rr:predicateObjectMap [ rr:predicate obo:BFO_0000051 ;
                            rr:objectMap [ rr:parentTriplesMap <Mapping:IAO_0020015> ;
                                           rr:joinCondition    [ rr:child  "\"PRENOM\"" ;
                                                                 rr:parent "\"PRENOM\"" ] ] ] .

# "HBW_0000022"@human name BFO_0000051@'has part' "IAO_0020017"@family name
<Mapping:HBW_0000022>
    rr:predicateObjectMap [ rr:predicate obo:BFO_0000051 ;
                            rr:objectMap [ rr:parentTriplesMap <Mapping:IAO_0020017> ;
                                           rr:joinCondition    [ rr:child  "\"NOM\"" ;
                                                                 rr:parent "\"NOM\"" ] ] ] .

# -- Propriétés de données
# "IAO_0020015"@personal name PHYSIO_0000100@'has value' "PRENOM"
<Mapping:IAO_0020015>
    rr:predicateObjectMap [ rr:predicate obo:PHYSIO_0000100 ;
                            rr:objectMap [ rr:column   "\"PRENOM\"" ;
                                           rr:datatype xsd:string ] ] .

# "IAO_0020017"@family name 'has value' "NOM"
<Mapping:IAO_0020017>
    rr:predicateObjectMap [ rr:predicate obo:PHYSIO_0000100 ;
                            rr:objectMap [ rr:column   "\"NOM\"" ;
                                           rr:datatype xsd:string ] ] .

# **** Héritage simple ****
# Aucune définition supplémentaire requise, puisque l'héritage simple est généré automatiquement.

