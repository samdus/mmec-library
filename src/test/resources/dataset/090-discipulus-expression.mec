from schema.table1
	natural join table2
	join table3 using (col1)
	join table4 on table4.col3 = table3.col2
	left join table5 on  table5.col3 = col1
	                 and table5.col4 = table3.col2
	                 and (   fn(col1) = fn2()
	                      OR fn(table3.col2) = schema.fn3(table4.col4)
	                      OR (    fn(4) = fn(fn2(schema.fn3(table3.col3)))
	                          AND col1 IS NOT NULL))
	                 and true
	right join table6 using(col1)
select col1, col2, table2.col3, fn(col1)
where table1.col6 = 3